{
  "name": "mapa-sinangoe",
  "version": "1.0.0",
  "description": "sinangoe interactive map",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "build": "webpack --config webpack.dev.js",
    "build:data": "./bin/generate-data",
    "build:screenshots": "./bin/generate-screenshots",
    "build:sw": "workbox injectManifest && bin/inject-cache-warmer",
    "build:js": "npm run build:map && npm run build:admin",
    "build:admin": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "build:map": "NODE_ENV=production browserify --debug -t [ sheetify/transform -u sheetify-nested ] -p [ css-extract -o dist/bundle.css ] -g unassertify -g envify -t yo-yoify -t es2020 -g uglifyify -p common-shakeify -p browser-pack-flat/plugin src/index.js | uglifyjs --source-map 'content=inline' -c comparisons=false -m -o dist/bundle.js",
    "build:static": "copyup 'static/**/*' dist && bin/ssr",
    "start:map": "budo src/index.js:bundle.js -d static -- -t [ sheetify/transform -u sheetify-nested]",
    "start:admin": "webpack-dev-server --config build/webpack.dev.js",
    "deploy": "npm run build && gh-pages -d dist" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/mapa-waorani.git"
  },
  "keywords": [
    "map",
    "waorani"
  ],
  "author": "Digital Democracy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digidem/mapa-waorani/issues"
  },
  "homepage": "https://github.com/digidem/mapa-waorani#readme",
  "dependencies": {
    "@digidem/attribution-control": "^1.0.0",
    "@ncwidgets/reorder": "^0.7.0",
    "@vimeo/player": "^2.6.1",
    "cross-env": "^7.0.2",
    "debug": "^3.1.0",
    "global": "^4.3.2",
    "lozad": "^1.4.0",
    "mapbox-gl": "^0.45.0",
    "mapbox-gl-toggle-control": "^1.0.5",
    "nanocomponent": "^6.5.2",
    "nanohtml": "^1.2.4",
    "nanomorph": "^5.1.3",
    "netlify-cms-app": "^2.12.9",
    "on-intersect": "^2.0.0",
    "on-load": "^3.4.0",
    "sheetify": "^6.1.1",
    "workbox-cli": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babelify": "^10.0.0",
    "browser-pack-flat": "^3.1.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "common-shakeify": "^0.4.6",
    "copy-webpack-plugin": "^5.1.1",
    "copyfiles": "^1.2.0",
    "css-extract": "^1.3.0",
    "envify": "^4.1.0",
    "es2020": "^1.1.9",
    "gh-pages": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^0.5.1",
    "puppeteer": "^1.4.0",
    "request": "^2.87.0",
    "sheetify": "^6.1.1",
    "sheetify-nested": "^1.0.2",
    "standard": "^10.0.3",
    "traverse": "^0.6.6",
    "uglify-js": "^3.3.27",
    "uglifyify": "^5.0.0",
    "unassertify": "^2.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "yo-yoify": "^3.7.3"
  }
}
